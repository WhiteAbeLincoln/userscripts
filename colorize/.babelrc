{
  "presets": [
    ["@babel/preset-env"],
    ["@babel/preset-typescript", {
      "isTSX": true,
      "allExtensions": true
    }]
  ],
  "plugins": [
    "@babel/plugin-syntax-import-meta",
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "babel-plugin-macros",
    ["@babel/plugin-transform-runtime", {
      "useESModules": true,
      "version": "^7.5.0"
    }]
  ]
}
